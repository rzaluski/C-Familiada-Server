<Page x:Class="Server.Table"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Server"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Table">
    <DockPanel LastChildFill="True">
        <StackPanel DockPanel.Dock="Left" Width="100px" Orientation="Horizontal">
            <Label Name="labelLeft" VerticalAlignment="Center" HorizontalAlignment="Left" Content="0"/>
        </StackPanel>
        <StackPanel DockPanel.Dock="Right" Orientation="Horizontal" Width="100px" FlowDirection="RightToLeft">
            <Label Name="labelRight" VerticalAlignment="Center" HorizontalAlignment="Right" Content="0"/>
        </StackPanel>
        <StackPanel DockPanel.Dock="Top">
            <Label Name="labelTop" Content="0" HorizontalAlignment="Center"/>
        </StackPanel>
        <StackPanel Name="stackPanelAnswers" Orientation="Vertical" HorizontalAlignment="Center">
        </StackPanel>
    </DockPanel>
</Page>
