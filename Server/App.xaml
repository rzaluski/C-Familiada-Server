<Application x:Class="Server.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Server"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="answers">
            <!--<Setter Property="TextElement.FontFamily" Value="Resources/subway-ticker.regular.ttf#Subway Ticker"/>-->
            <!--<Setter Property="TextElement.FontFamily" Value="Resources/advanced-led-board-7.regular#Advanced LED Board-7"/>-->
            <Setter Property="TextElement.FontFamily" Value="Resources/trs-million.regular.ttf#TRS Million"/>
            <Setter Property="TextElement.FontSize" Value="50"/>
            <Setter Property="TextElement.Foreground" Value="Yellow"/>
        </Style>
        <Style x:Key="points">
            <!--<Setter Property="TextElement.FontFamily" Value="Resources/subway-ticker.regular.ttf#Subway Ticker"/>-->
            <!--<Setter Property="TextElement.FontFamily" Value="Resources/advanced-led-board-7.regular#Advanced LED Board-7"/>-->
            <Setter Property="TextElement.FontFamily" Value="Resources/trs-million.regular.ttf#TRS Million"/>
            <Setter Property="TextElement.FontSize" Value="70"/>
            <Setter Property="TextElement.Foreground" Value="Yellow"/>
        </Style>
    </Application.Resources>
</Application>
